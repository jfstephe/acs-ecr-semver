# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*.sh]
end_of_line = lf
insert_final_newline = true
# 2 space indentation
[*.ts]
indent_style = space
indent_size = 2
# 2 space indentation
[*.js]
indent_style = space
indent_size = 2
# 2 space indentation
[*.json]
indent_style = space
indent_size = 2